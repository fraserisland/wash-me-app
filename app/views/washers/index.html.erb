<p id="notice"><%= notice %></p>

<h1>Washers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Cost</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @washers.each do |washer| %>
      <tr>
        <td><%= washer.user %></td>
        <td><%= washer.cost %></td>
        <td><%= washer.address %></td>
        <td><%= washer.latitude %></td>
        <td><%= washer.longitude %></td>
        <td><%= link_to 'Show', washer %></td>
        <td><%= link_to 'Edit', edit_washer_path(washer) %></td>
        <td><%= link_to 'Destroy', washer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Washer', new_washer_path %>
